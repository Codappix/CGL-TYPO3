{
    "name": "codappix/cgl-typo3",
    "description": "Contains PHP_CodeSniffer rules for TYPO3 that we like. Uses CGL-PHP as base rules.",
    "type": "phpcodesniffer-standard",
    "license": "GPL-2.0+",
    "keywords": [
        "phpcs",
        "cgl",
        "php",
        "typo3",
        "cms"
    ],
    "homepage": "https://github.com/Codappix/CGL-TYPO3",
    "authors": [
        {
            "name": "Justus Moroni",
            "email": "developer@leonmrni.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/Codappix/CGL-TYPO3/issues/",
        "source": "https://github.com/Codappix/CGL-TYPO3/"
    },
    "autoload": {
        "psr-4": {
            "Codappix\\CDXTYPO3\\": "CDXTYPO3/"
        }
    },
    "require": {
        "codappix/cgl-php": "^3.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4"
    },
    "suggest": {
        "dealerdirect/phpcodesniffer-composer-installer": "Auto-installs the standard to phpcs."
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "4.0.x-dev"
        }
    }
}
